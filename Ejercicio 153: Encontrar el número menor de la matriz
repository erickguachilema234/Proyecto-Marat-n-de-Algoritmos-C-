#include <iostream>
using namespace std;

int main() {
    int mat[3][3], menor;
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 3; j++)
            cin >> mat[i][j];

    menor = mat[0][0];
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 3; j++)
            if (mat[i][j] < menor) menor = mat[i][j];

    cout << "Menor: " << menor;
    return 0;
}
