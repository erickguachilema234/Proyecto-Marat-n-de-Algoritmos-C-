#include <iostream>
#include <cctype>
using namespace std;

int main() {
    char caracter;
    cout << "Ingrese un caracter: ";
    cin >> caracter;

    if (isdigit(caracter))
        cout << "Es un numero." << endl;
    else if (isalpha(caracter))
        cout << "Es una letra." << endl;
    else
        cout << "Es otro tipo de caracter." << endl;

    return 0;
}
