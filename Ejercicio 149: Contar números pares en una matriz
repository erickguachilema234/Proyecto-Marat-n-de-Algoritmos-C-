#include <iostream>
using namespace std;

int main() {
    int mat[3][3], cont = 0;
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 3; j++) {
            cin >> mat[i][j];
            if (mat[i][j] % 2 == 0) cont++;
        }
    cout << "Cantidad de pares: " << cont;
    return 0;
}
