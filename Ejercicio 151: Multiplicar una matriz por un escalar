#include <iostream>
using namespace std;

int main() {
    int mat[3][3], escalar;
    cout << "Ingrese matriz 3x3:\n";
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 3; j++)
            cin >> mat[i][j];

    cout << "Ingrese escalar: ";
    cin >> escalar;

    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 3; j++)
            mat[i][j] *= escalar;

    cout << "Resultado:\n";
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++)
            cout << mat[i][j] << " ";
        cout << endl;
    }
    return 0;
}
